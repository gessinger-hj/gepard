#!/usr/bin/env node
process.env.client = "true" ;
process.env.action = "execute" ;
var i = 2
if ( process.argv.length > 2 )
{
	if ( process.argv[2].indexOf ( "--" ) === 0 )
	{
		i++ ;
	}
}
if ( process.argv.length > i ) process.env.cmd = process.argv[2] ;
new (require ( "../src/Admin" ))().main() ;
